import http from 'http';
import assert from 'assert';

import '../index.js';

const url = "http://localhost:8000";


describe('Node Server', () => {

  describe('Node Server', () => {
  it('should return 200', done => {
    http.get(url, res => {
      assert.equal(200, res.statusCode);
      done();
    });
  });
});

